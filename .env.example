# ===========================
# MediSecure - Environment Variables
# ===========================

# PostgreSQL (Patients & Keycloak)
POSTGRES_USER=medisecure_user
POSTGRES_PASSWORD=change_me_in_production
POSTGRES_DB=medisecure_patients

# MongoDB (Appointments)
MONGO_USER=medisecure_admin
MONGO_PASSWORD=change_me_in_production

# MariaDB (Billing)
MARIADB_ROOT_PASSWORD=change_me_in_production
MARIADB_USER=medisecure_billing
MARIADB_PASSWORD=change_me_in_production

# MinIO (Documents Storage)
MINIO_ROOT_USER=medisecure_admin
MINIO_ROOT_PASSWORD=change_me_in_production_min12chars

# Redis (Cache)
REDIS_PASSWORD=change_me_in_production

# RabbitMQ (Message Queue)
RABBITMQ_USER=medisecure
RABBITMQ_PASSWORD=change_me_in_production

# Keycloak (Authentication)
KEYCLOAK_ADMIN_PASSWORD=change_me_in_production

# Grafana (Monitoring)
GRAFANA_PASSWORD=change_me_in_production

# Security
SECRET_KEY=change_this_secret_key_in_production_minimum_32_characters_required

# Frontend
VITE_API_BASE_URL=http://localhost:8000/api
VITE_KEYCLOAK_URL=http://localhost:8080

